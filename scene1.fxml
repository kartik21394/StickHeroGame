<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.apgame.StickHero">
   <children>
      <ImageView fitHeight="800.0" fitWidth="806.0" layoutX="-2.0" pickOnBounds="true">
         <image>
            <Image url="@MainPage.png" />
         </image>
      </ImageView>
      <Button layoutX="337.0" layoutY="501.0" mnemonicParsing="false" onAction="#setGame" prefHeight="35.0" prefWidth="122.0" style="-fx-background-color: orange;" text="Start" textAlignment="CENTER" textFill="#f8ff00">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
      <Button layoutX="339.0" layoutY="656.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="35.0" prefWidth="122.0" style="-fx-background-color: orange;" text="Exit" textAlignment="CENTER" textFill="#f8ff00">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
      <Button layoutX="339.0" layoutY="580.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="35.0" prefWidth="122.0" style="-fx-background-color: orange;" text="Saved Game" textAlignment="CENTER" textFill="#f8ff00">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
