<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.apgame.StickHero">
   <children>
      <ImageView fitHeight="800.0" fitWidth="806.0" layoutX="-2.0" pickOnBounds="true">
         <image>
            <Image url="@bg.jpg" />
         </image>
      </ImageView>
      <Button layoutX="265.0" layoutY="333.0" mnemonicParsing="false" onAction="#savegame" prefHeight="66.0" prefWidth="272.0" style="-fx-background-color: orange;" text="Save Game &amp; Exit" textFill="#eeff00">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button layoutX="266.0" layoutY="425.0" mnemonicParsing="false" onAction="#mainmenu" prefHeight="66.0" prefWidth="272.0" style="-fx-background-color: orange;" text="Main Menu " textFill="#eeff00">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#resumeGame" prefHeight="52.0" prefWidth="91.0" style="-fx-background-color: none; -fx-border-color: black;" text="Back">
         <font>
            <Font size="23.0" />
         </font></Button>
   </children>
</AnchorPane>
